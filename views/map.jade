extends layout

block content
  .container
    #map
  script(type='text/javascript').
         var map = L.map('map').setView([48.871764,2.385085], 15);
         
         L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
             attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
         }).addTo(map);
